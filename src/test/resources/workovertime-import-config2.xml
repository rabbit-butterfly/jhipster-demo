<?xml version="1.0" encoding="ISO-8859-1"?>
<workbook>
    <worksheet name="test">
        <loop startRow="1" endRow="1" items="simples" var="simple"
            varType="local.tux.app.web.rest.dto.oa.WorkOvertimeRow">

            <section startRow="1" endRow="1">
                <mapping row="2" col="0">simple.mail</mapping>
                <mapping row="2" col="1">simple.userName</mapping>
                <mapping row="2" col="2">simple.departName</mapping>
                <mapping row="2" col="3">simple.checkinTime</mapping>
                <mapping row="2" col="4">simple.checkoutTime</mapping> 
                <mapping row="2" col="5">simple.checkdate</mapping> 
                <mapping row="2" col="6">simple.status</mapping> 
            </section>
            <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0" />
                </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>
</workbook>