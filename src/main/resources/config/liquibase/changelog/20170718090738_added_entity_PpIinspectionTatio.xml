            </column>

            <column name="ut_grade" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="mt_rate" type="varchar(19)">
                <constraints nullable="true" />
            </column>

            <column name="mt_standard" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="mt_grade" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="create_time" type="timestamp">
                <constraints nullable="false" />
            </column>

            <column name="del_flag" type="integer">
                <constraints nullable="true" />
            </column>

            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here, do not remove-->
        </createTable>
        <dropDefaultValue tableName="pp_inspection_ratio" columnName="create_time" columnDataType="datetime"/>
        
    </changeSet>
</databaseChangeLog>
