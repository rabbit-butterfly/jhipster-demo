            <column name="rt_rate_gd" type="varchar(19)">
                <constraints nullable="true" />
            </column>

            <column name="rt_standard" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="rt_grade" type="varchar(50)">
