<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <property name="now" value="now()" dbms="h2"/>

    <property name="now" value="now()" dbms="mysql"/>
    <property name="autoIncrement" value="true"/>

    <property name="floatType" value="float4" dbms="postgresql, h2"/>
    <property name="floatType" value="float" dbms="mysql, oracle, mssql"/>

    <!--
        Added the entity Project.
    -->
    <changeSet id="20170712103801-1" author="jhipster">
        <createTable tableName="project">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="project_comp_id" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="project_comp_name" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="project_bame" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="project_desc" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="project_status" type="integer">
                <constraints nullable="true" />
            </column>

            <column name="supervisor_comp_id" type="bigint">
                <constraints nullable="true" />
            </column>

            <column name="supervisor_comp_name" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="contractor_comp_id" type="bigint">
                <constraints nullable="true" />
            </column>

            <column name="contractor_comp_name" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="start_date" type="timestamp">
                <constraints nullable="true" />
            </column>

            <column name="end_date" type="timestamp">
                <constraints nullable="true" />
            </column>

            <column name="created_by" type="bigint">
                <constraints nullable="true" />
            </column>

            <column name="created_time" type="timestamp">
                <constraints nullable="true" />
            </column>

            <column name="modify_by" type="bigint">
                <constraints nullable="true" />
            </column>

            <column name="modify_time" type="timestamp">
                <constraints nullable="true" />
            </column>

            <column name="del_flag" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <column name="remark" type="varchar(255)">
                <constraints nullable="true" />
            </column>

            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here, do not remove-->
        </createTable>
        <dropDefaultValue tableName="project" columnName="start_date" columnDataType="datetime"/>
        <dropDefaultValue tableName="project" columnName="end_date" columnDataType="datetime"/>
        <dropDefaultValue tableName="project" columnName="created_time" columnDataType="datetime"/>
        <dropDefaultValue tableName="project" columnName="modify_time" columnDataType="datetime"/>
        
    </changeSet>
</databaseChangeLog>
