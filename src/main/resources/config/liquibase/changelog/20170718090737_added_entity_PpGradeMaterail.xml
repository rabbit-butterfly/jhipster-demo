<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <property name="now" value="now()" dbms="h2"/>

    <property name="now" value="now()" dbms="mysql"/>
    <property name="autoIncrement" value="true"/>

    <property name="floatType" value="float4" dbms="postgresql, h2"/>
    <property name="floatType" value="float" dbms="mysql, oracle, mssql"/>

    <!--
        Added the entity PpGradeMaterail.
    -->
    <changeSet id="20170718090737-1" author="jhipster">
        <createTable tableName="pp_grade_materail">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="comp_id" type="varchar(19)">
                <constraints nullable="false" />
            </column>

            <column name="proj_id" type="varchar(19)">
                <constraints nullable="false" />
            </column>

            <column name="material" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="pp_class" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="jsize_min" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="jsize_max" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="wall_thick_code" type="varchar(64)">
                <constraints nullable="true" />
            </column>

            <column name="material_type" type="varchar(64)">
                <constraints nullable="true" />
            </column>

            <column name="material_category" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="create_time" type="timestamp">
                <constraints nullable="false" />
            </column>

            <column name="del_flag" type="integer">
                <constraints nullable="true" />
            </column>

            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here, do not remove-->
        </createTable>
        <dropDefaultValue tableName="pp_grade_materail" columnName="create_time" columnDataType="datetime"/>
        
    </changeSet>
</databaseChangeLog>
