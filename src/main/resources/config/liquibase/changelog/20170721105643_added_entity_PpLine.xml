<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <property name="now" value="now()" dbms="h2"/>

    <property name="now" value="now()" dbms="mysql"/>
    <property name="autoIncrement" value="true"/>

    <property name="floatType" value="float4" dbms="postgresql, h2"/>
    <property name="floatType" value="float" dbms="mysql, oracle, mssql"/>

    <!--
        Added the entity PpLine.
    -->
    <changeSet id="20170721105643-1" author="jhipster">
        <createTable tableName="pp_line">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="comp_id" type="varchar(19)">
                <constraints nullable="false" />
            </column>

            <column name="proj_id" type="varchar(19)">
                <constraints nullable="false" />
            </column>

            <column name="area_id" type="varchar(19)">
                <constraints nullable="false" />
            </column>

            <column name="line_num" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="line_version" type="varchar(20)">
                <constraints nullable="true" />
            </column>

            <column name="jhi_size" type="varchar(10)">
                <constraints nullable="true" />
            </column>

            <column name="serv" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="class_spec" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="line_from" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="line_to" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="operate_pressure" type="varchar(20)">
                <constraints nullable="true" />
            </column>

            <column name="operate_temperature" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="line_level" type="varchar(10)">
                <constraints nullable="true" />
            </column>

            <column name="line_code" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="design_pressure" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="design_temperature" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="kind_level" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="inspection_code" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="insulation_code" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="insulation_high" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="paint_code" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="pi_diagram" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="test_pressure" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="test_material" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="jacketed_pipe" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="remark" type="varchar(500)">
                <constraints nullable="true" />
            </column>

            <column name="create_time" type="timestamp">
                <constraints nullable="false" />
            </column>

            <column name="del_flag" type="integer">
                <constraints nullable="true" />
            </column>

            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here, do not remove-->
        </createTable>
        <dropDefaultValue tableName="pp_line" columnName="create_time" columnDataType="datetime"/>
        
    </changeSet>
</databaseChangeLog>
