<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <property name="now" value="now()" dbms="h2"/>

    <property name="now" value="now()" dbms="mysql"/>
    <property name="autoIncrement" value="true"/>

    <property name="floatType" value="float4" dbms="postgresql, h2"/>
    <property name="floatType" value="float" dbms="mysql, oracle, mssql"/>

    <!--
        Added the entity ProjectWelder.
    -->
    <changeSet id="20170718090753-1" author="jhipster">
        <createTable tableName="project_welder">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="comp_id" type="varchar(19)">
                <constraints nullable="false" />
            </column>

            <column name="code" type="varchar(20)">
                <constraints nullable="true" />
            </column>

            <column name="name" type="varchar(10)">
                <constraints nullable="false" />
            </column>

            <column name="mobille" type="varchar(16)">
                <constraints nullable="true" />
            </column>

            <column name="identity_card" type="varchar(18)">
                <constraints nullable="true" />
            </column>

            <column name="work_term" type="varchar(10)">
                <constraints nullable="true" />
            </column>

            <column name="unit_name" type="varchar(50)">
                <constraints nullable="true" />
            </column>

            <column name="create_time" type="timestamp">
                <constraints nullable="false" />
            </column>

            <column name="del_flag" type="integer">
                <constraints nullable="true" />
            </column>

            <column name="remark" type="varchar(500)">
                <constraints nullable="true" />
            </column>

            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here, do not remove-->
        </createTable>
        <dropDefaultValue tableName="project_welder" columnName="create_time" columnDataType="datetime"/>
        
    </changeSet>
</databaseChangeLog>
